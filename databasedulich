CREATE TABLE `dulich`.`employee` (
  `id` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `dateofbirth` DATE NULL,
  `gender` TINYINT(1) NULL,
  `email` VARCHAR(45) NULL,
  `phone` VARCHAR(15) NULL,
  `address` VARCHAR(256) NULL,
  `id_account` INT NOT NULL,
  PRIMARY KEY (`id`));
  
  
CREATE TABLE `dulich`.`customer` (
  `id` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `dateofbirth` DATE NULL,
  `gender` TINYINT(1) NULL,
  `email` VARCHAR(45) NULL,
  `phone` VARCHAR(15) NULL,
  `address` VARCHAR(256) NULL,
  `id_account` INT NOT NULL,
  PRIMARY KEY (`id`));
  
  
CREATE TABLE `dulich`.`managers` (
  `id` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `dateofbirth` DATE NULL,
  `gender` TINYINT(1) NULL,
  `email` VARCHAR(45) NULL,
  `phone` VARCHAR(15) NULL,
  `address` VARCHAR(256) NULL,
  `id_account` INT NOT NULL,
  PRIMARY KEY (`id`));
  
  
CREATE TABLE `dulich`.`account` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(45) NULL,
  `password` VARCHAR(45) NULL,
  `role` VARCHAR(10) NULL,
  PRIMARY KEY (`id`));


ALTER TABLE `dulich`.`customer` 
ADD INDEX `fkcustomer_account_idx` (`id_account` ASC) VISIBLE;
;
ALTER TABLE `dulich`.`customer` 
ADD CONSTRAINT `fkcustomer_account`
  FOREIGN KEY (`id_account`)
  REFERENCES `dulich`.`account` (`id`)
  ON DELETE NO ACTION
  ON UPDATE NO ACTION;
  
ALTER TABLE `dulich`.`employee` 
ADD INDEX `fkemployee_account_idx` (`id_account` ASC) VISIBLE;
;
ALTER TABLE `dulich`.`employee` 
ADD CONSTRAINT `fkemployee_account`
  FOREIGN KEY (`id_account`)
  REFERENCES `dulich`.`account` (`id`)
  ON DELETE NO ACTION
  ON UPDATE NO ACTION;
  
ALTER TABLE `dulich`.`managers` 
ADD INDEX `fkmanagers_account_idx` (`id_account` ASC) VISIBLE;
;
ALTER TABLE `dulich`.`managers` 
ADD CONSTRAINT `fkmanagers_account`
  FOREIGN KEY (`id_account`)
  REFERENCES `dulich`.`account` (`id`)
  ON DELETE NO ACTION
  ON UPDATE NO ACTION;
  
  
CREATE TABLE `dulich`.`place` (
  `id` INT NOT NULL,
  `name` VARCHAR(256) NULL,
  `description` VARCHAR(500) NULL,
  `image` VARCHAR(45) NULL,
  `local` VARCHAR(100) NULL,
  PRIMARY KEY (`id`));

  
  

  
  

  